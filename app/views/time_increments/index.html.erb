<h1>Listing time_increments</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Description</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @time_increments.each do |time_increment| %>
      <tr>
        <td><%= time_increment.name %></td>
        <td><%= time_increment.description %></td>
        <td><%= link_to 'Show', time_increment %></td>
        <td><%= link_to 'Edit', edit_time_increment_path(time_increment) %></td>
        <td><%= link_to 'Destroy', time_increment, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Time increment', new_time_increment_path %>
